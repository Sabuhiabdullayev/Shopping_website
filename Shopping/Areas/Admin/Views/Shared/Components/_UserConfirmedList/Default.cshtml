@model List<AppUser>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
  <link rel="stylesheet" href="~/css/VipPremiumAllList/VipPremiumAllList.css">
  <div class="VipPremiumAllListTopbarDiv">
        <div class="VipPremiumAllListTopbarAllList">
<div class="VipPremiumAllListTopbar">
    <tr >
        <th ><button class="btn btn-outline-success m-4" id="ApprovedBtn" >Icon Təsdiqlənmiş Olan Hesablar</button></th>
        <th ><button class="btn btn-outline-primary m-4" id="ConfirmationpancakeBtn" > Icon Təsdiqlənmə Yoxlanılan Hesablar</button></th>
        <th ><button class="btn btn-outline-danger m-4"  id="NotapprovedBtn">Icon Təsdiqlənməmiş Hesablar</button></th>
    </tr>
</div>
<div class="VipPremiumAllList" id="AllListDiv" >
    <table class="table table-bordered">
		<tr>
		<th>#</th>
		<th>Ad Soyad</th>
		<th>Tel</th>
		<th>Açma Tarix</th>
		<th>Icon Status</th>
		<th></th>
		<th>Hesaba Göz At</th>
		</tr>

		@foreach(var x in Model)
	{
		@if(x.confirmed == "Təsdiqləndi")
		{
			<tr class="table table-success">
			<td>@x.Id</td>
			<td>@x.Name @x.SurName</td>
			<td>@x.PhoneNumber</td>
			<td>@(((DateTime)x.AppUserDate).ToString("dd-MMM-yyyy"))</td>
			<td>@x.confirmed </td>
			<td> <a href="/Admin/User/UserStatusGive/@x.Id" class="btn btn-outline-primary">Status Ver</a></td>
			<td><a href="/User/Profile/@x.Id" class="btn btn-outline-info">Göz At</a></td>
		</tr>
		}
		@if(x.confirmed == "Yoxlanılır")
		{
			<tr class="table table-warning">
			<td>@x.Id</td>
			<td>@x.Name @x.SurName</td>
			<td>@x.PhoneNumber</td>
			<td>@(((DateTime)x.AppUserDate).ToString("dd-MMM-yyyy"))</td>
			<td>@x.confirmed </td>
			<td> <a href="/Admin/User/UserStatusGive/@x.Id" class="btn btn-outline-primary">Status Ver</a></td>
			<td><a href="/User/Profile/@x.Id" class="btn btn-outline-info">Göz At</a></td>
		</tr>
		}	
		@if(x.confirmed == "Təsdiqlənmədi")
		{
			<tr class="table table-danger">
			<td>@x.Id</td>
			<td>@x.Name @x.SurName</td>
			<td>@x.PhoneNumber</td>
			<td>@(((DateTime)x.AppUserDate).ToString("dd-MMM-yyyy"))</td>
			<td>@x.confirmed </td>
			<td> <a href="/Admin/User/UserStatusGive/@x.Id" class="btn btn-outline-primary">Status Ver</a></td>
			<td><a href="/User/Profile/@x.Id" class="btn btn-outline-info">Göz At</a></td>
		</tr>
		}
		
	}
</table>

@model List<AppUser>
@{

    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="mr-4 ml-4">
<h5>İstifadəçi Listəsi</h5>
<br />
<table class="table table-bordered">
	<tr>
		<th>#</th>
		<th>Istifadeci Adi</th>
		<th>Rol Ver</th>
    </tr>
	@foreach(var item in Model)
	{
		<tr>
		<td>@item.Id</td>
		<td>@item.UserName</td>
	<td><a href="/Admin/AdminRoles/AssignRole/@item.Id" class="btn btn-outline-info">Rol Ver</a></td>

    </tr>

	}	
</table>
</div>